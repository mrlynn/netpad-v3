{
  "name": "@netpad/workflows",
  "version": "0.1.0",
  "description": "Programmatically trigger and manage NetPad workflows from your applications. Type-safe TypeScript client for NetPad workflow APIs.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "netpad",
    "workflows",
    "automation",
    "typescript",
    "api-client",
    "workflow-automation",
    "orchestration"
  ],
  "author": "MongoDB",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/netpad.git",
    "directory": "packages/workflows"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
